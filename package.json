{
    "name": "react17-webpack5-cli",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "export IS_CLIENT=false; export NODE_OPTIONS=--max_old_space_size=8192 && webpack serve --config ./config/webpack.config.js",
        "build:dll": "rm -rf dll/*;export NODE_ENV=production;node ./node_modules/webpack/bin/webpack.js --config config/webpack.config.dll.js",
        "build": "export IS_CLIENT=false; webpack --mode production --config ./config/webpack.config.js",
        "build:client": "export IS_CLIENT=true; webpack --mode production --config ./config/webpack.config.js",
        "analyze": "webpack --mode production --analyze --config ./config/webpack.config.js",
        "lint": "npx eslint --ext .jsx,.ts,.tsx ./src/",
        "fix": "npx prettier --write ./src/*.{tsx,ts,less}"
    },
    "keywords": [],
    "author": "",
